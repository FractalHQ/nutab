		"@sveltejs/adapter-static": "1.0.0-next.42",
		"@sveltejs/adapter-vercel": "1.0.0-next.72",
		"@sveltejs/kit": "1.0.0-next.474",
		"@sveltejs/vite-plugin-svelte": "^1.0.5",
		"@types/node": "^18.7.15",
		"@types/sjcl": "^1.0.30",
		"autoprefixer": "^10.4.8",
