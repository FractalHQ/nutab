{
	"name": "nutab",
	"version": "0.0.1",
	"description": "A better new-tab browser extension",
	"keywords": [
		"svelte",
		"sveltekit",
		"newtab",
		"extension",
		"chrome",
		"browser"
	],
	"type": "module",
	"scripts": {
		"dev": "run-p dev:*",
