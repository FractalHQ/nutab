{
	"browser_action": {
		"default_title": "Nutab",
		"default_popup": "index.html"
	},
	"description": "A better new tab for developers.",
	"author": "Fractal <braden@fractal-hq.com>",
	"name": "Nutab",
	"version": "0.1",
	"manifest_version": 3,
	"chrome_url_overrides": {
		"newtab": "index.html"
	},
	"permissions": ["storage"],
	"host_permissions": ["https://*/*"],
	"icons": {
		"16": "/favicon-16x16.png",
		"32": "/favicon-32x32.png",
		"192": "/android-chrome-192x192.png",
		"512": "/android-chrome-512x512.png"
	}
}
